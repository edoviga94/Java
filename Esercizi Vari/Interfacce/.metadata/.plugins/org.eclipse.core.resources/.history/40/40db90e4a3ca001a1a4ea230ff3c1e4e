
public class Addition extends Stampa implements Operazione {
	
	private double op1;
	private double op2;
	
	
	
	

	public Addition(double op1, double op2) {
		super();
		this.op1 = op1;
		this.op2 = op2;
	}
	
	@Override
	public double eseguiOp(double a, double b) {
		return a+b;
	}
	
	





	@Override
	public void stampa() {
		System.out.println("Il risultato dell'operazione è: "+eseguiOp());

	}

}
